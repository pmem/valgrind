#! /bin/sh

dir=`dirname $0`

$dir/../../../tests/filter_stderr_basic |

# Filter out glibc debuginfo if installed.
perl -p -e "s/\(syscall-template.S:[0-9]*\)/(in \/...libc...)/" |
perl -p -e "s/\(socket.S:[0-9]*\)/(in \/...libc...)/"

exit 0
